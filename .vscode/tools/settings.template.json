{
    "files.associations": {
        "*.tpp": "cpp",
        "*.kit": "toml",
        "*.rst": "restructuredtext"
    },
    "editor.rulers": [150],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/_isaac_sim/**": true,
        "**/_compiler/**": true
    },
    "spellright.language": [
        "en-US-10-1."
    ],
    "spellright.documentTypes": [
        "markdown",
        "latex",
        "plaintext",
        "cpp",
        "asciidoc",
        "python",
        "restructuredtext"
    ],
    "cSpell.words": [
        "literalinclude",
        "linenos",
        "instanceable",
        "isaacSim",
        "jacobians",
        "pointcloud",
        "ridgeback",
        "rllib",
        "robomimic",
        "teleoperation",
        "xform",
        "numpy",
        "tensordict",
        "flatcache",
        "physx",
        "dpad",
        "gamepad",
        "linspace",
        "upsampled",
        "downsampled",
        "arange",
        "discretization",
        "trimesh",
        "uninstanceable"
    ],
    "python.languageServer": "Pylance",
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": ["--line-length", "150"],
    "python.linting.pylintEnabled": false,
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--max-line-length=180"
    ],
    "autoDocstring.docstringFormat": "google",
    "autoDocstring.guessTypes": true,
    "python.defaultInterpreterPath": "",
    "ros.distro": "noetic",
    "[python]": {
        "editor.tabSize": 4
    },
    "[restructuredtext]": {
        "editor.tabSize": 2
    },
    "python.analysis.extraPaths": []
}
